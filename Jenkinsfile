pipeline{
    agent any
        stages{
          stage("Build"){
                  steps{
                    echo "Build my jenkins pipeline..."
                        }
                    }
          stage("test"){
                  steps{
                    echo "Testing my jenkins pipeline..."
                        }
                    }
          stage("deploy"){
                  steps{
                    echo "deploy my jenkins pipeline..."
                        }
                    }
            }
      }
